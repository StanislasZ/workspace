package Ð¡¹¦ÄÜ;

import java.util.Scanner;

public class Problem1003 {

	public static void main(String[] args) {
		
		Scanner in=new Scanner(System.in);
		
		int T=in.nextInt();
		System.out.println("T="+T);
		int flag=0;
		while(flag<T){
			
			
			int num=in.nextInt();
			int[] arr=new int[num];
			for(int i=0;i<arr.length;i++){
				arr[i]=in.nextInt();
				System.out.println("arr["+i+"]="+arr[i]);
			}
			
			
			
		}
		
		
		

	}

}
