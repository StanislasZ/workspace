package demo;

import domain.DoublyLinkedList;

public class Demo99 {

	public static void main(String[] args) {

		
		DoublyLinkedList dll=new DoublyLinkedList();
	
		dll.add(1);
		dll.add(2);
		dll.add(3);
		
		dll.add(1, 100);
		dll.add(1, 200);
		
		dll.add(4,500);
		
		for(int i=0;i<dll.getSize();i++){
			System.out.println(dll.getValue(i));
		}
		
		dll.remove(3);
		
		for(int i=0;i<dll.getSize();i++){
			System.out.println(dll.getValue(i));
		}
		
		
